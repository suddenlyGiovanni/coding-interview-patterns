{
  "extends": [
	"@tsconfig/node23/tsconfig.json",
	"@tsconfig/node-ts/tsconfig.json",
	"@tsconfig/strictest/tsconfig.json"
  ],
  "compilerOptions": {
	"skipLibCheck": true,
	"isolatedModules": true,
	"moduleDetection": "force"
  },
  "include": [
	"**/*.ts",
	"**/*.tsx"
  ],
  "exclude": [
	"node_modules"
  ]
}
